{
    "name": "resellpiacenza/woo-import",
    "description": "Golden Sneakers to WooCommerce Product Importer with Italian localization",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "ResellPiacenza",
            "email": "dev@resellpiacenza.it"
        }
    ],
    "require": {
        "php": ">=7.4",
        "automattic/woocommerce": "^3.0",
        "monolog/monolog": "^2.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "ResellPiacenza\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "import-wc": "php bin/import-wc",
        "import-wc:dry": "php bin/import-wc --dry-run",
        "gs:sync": "./gs-sync.sh",
        "gs:sync:dry": "./gs-sync.sh --dry-run",
        "gs:transform": "php bin/gs-transform",
        "gs:taxonomies": "php bin/prepare-taxonomies --from-gs",
        "gs:media": "php bin/prepare-media --from-gs",
        "kicksdb:import": "php bin/import-kicksdb",
        "kicksdb:import:dry": "php bin/import-kicksdb --dry-run",
        "kicksdb:transform": "php bin/import-kicksdb --transform-only --save-feed",
        "bulk-upload": "php bin/bulk-upload",
        "pricing:reconcile": "php bin/pricing-reconcile",
        "pricing:reconcile:dry": "php bin/pricing-reconcile --dry-run",
        "pricing:reconcile:verbose": "php bin/pricing-reconcile --verbose"
    }
}