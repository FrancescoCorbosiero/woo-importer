{
    "name": "resellpiacenza/woo-import",
    "description": "Golden Sneakers to WooCommerce Product Importer with Italian localization",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "ResellPiacenza",
            "email": "dev@resellpiacenza.it"
        }
    ],
    "require": {
        "php": ">=7.4",
        "automattic/woocommerce": "^3.0",
        "monolog/monolog": "^2.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "ResellPiacenza\\": "src/",
            "WooImporter\\Database\\": "database/",
            "WooImporter\\Models\\": "models/",
            "WooImporter\\Services\\": "services/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "import": "php import.php",
        "import:dry": "php import.php --dry-run",
        "import:test": "php import.php --dry-run --limit=5",
        "images": "php import-images.php",
        "images:test": "php import-images.php --limit=5",
        "sync": "php sync-all.php",
        "sync:dry": "php sync-all.php --dry-run",
        "sync:feed": "php sync-feed-to-db.php",
        "sync:woo": "php sync-db-to-woo.php",
        "webhooks": "php process-webhooks.php",
        "webhooks:stats": "php process-webhooks.php --stats"
    }
}